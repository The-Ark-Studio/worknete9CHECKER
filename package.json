{
  "name": "ehm-official",
  "version": "0.1.0",
  "author": "The Ark Studio",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.0.1",
  "scripts": {
    "prepare": "husky",
    "lint:check": "npm run lint || npm run lint:fix",
    "lint": "pnpm csf && pnpm cqf",
    "lint:fix": "eslint --fix-dry-run $(git diff --name-only --diff-filter=d HEAD -- '**/*.js' '**/*.jsx' '**/*.ts' '**/*.tsx' | xargs)",
    "dev": "refine dev",
    "build": "tsc && refine build --mode production",
    "quick-build": "refine build --mode production",
    "cs": "prettier --config .prettierrc --check . \"{**/*,*}.{html, js,ts,jsx,tsx,json,scss,less}\"",
    "csf": "prettier --config .prettierrc --write . \"{**/*,*}.{html, js,ts,jsx,tsx,json,scss,less}\"",
    "cq": "eslint .  \"{**/*,*}.{ts,tsx}\" --max-warnings=0",
    "cqf": "eslint .  \"{**/*,*}.{ts,tsx}\" --max-warnings=0 --fix",
    "verify": "pnpm cs && pnpm cq && pnpm compile",
    "certify": "refine build && pnpm verify",
    "afm": "pnpm certify && refine build",
    "compile": "tsc",
    "preview": "refine start",
    "refine": "refine",
    "release": "release",
    "uplib": "taze major -I"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@pankod/refine-core": "^3.103.0",
    "@refinedev/antd": "^5.37.6",
    "@refinedev/cli": "^2.16.29",
    "@refinedev/core": "^4.49.0",
    "@refinedev/devtools": "^1.1.37",
    "@refinedev/inferencer": "^4.6.0",
    "@refinedev/kbar": "^1.3.8",
    "@refinedev/react-router-v6": "^4.5.7",
    "@refinedev/simple-rest": "^5.0.4",
    "@uiw/react-md-editor": "^4.0.4",
    "antd": "^5.16.2",
    "axios": "^1.6.8",
    "axxios": "^0.1.0",
    "i18next": "^23.11.2",
    "query-string": "7",
    "react": "^18.2.0",
    "react-css-modules": "^4.7.11",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.0.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "8.57.0",
    "eslint-config-love": "^47.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.2.1",
    "eslint-plugin-project-structure": "^1.4.7",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "taze": "^0.13.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "vite": "^5.2.8",
    "vite-plugin-eslint-report": "^1.0.1",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-package-version": "^1.1.0",
    "vite-plugin-pwa": "0.16.7",
    "vite-plugin-qrcode": "^0.2.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "refine": {
    "projectId": "ANRfnA-imNw4M-JNXP8l"
  }
}